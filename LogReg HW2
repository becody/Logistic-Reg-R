	class DDA DIRDEP SAV ATM CD IRA LOC INV 
		  ILS MM MTG CC SDB HMOWN MOVED INAREA INS BRANCH RES;
	model INS = DDA DIRDEP SAV ATM CD IRA LOC INV 
		  ILS MM MTG CC SDB HMOWN MOVED INAREA INS BRANCH RES;
run;

proc print data=loghw2.dummy; run;


